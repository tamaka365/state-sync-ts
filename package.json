{
  "name": "state-sync-ts",
  "version": "0.0.1",
  "author": "tamaka365 <tamaka365@gmail.com>",
  "license": "MIT",
  "private": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "default": "./dist/*.cjs",
      "types": "./dist/types/*.d.ts"
    }
  },
  "type": "module",
  "files": [
    "dist/",
    "dist/types/"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build && pnpm build:types",
    "preview": "vite preview",
    "build:types": "tsc lib/index.ts --target esnext --declaration --emitDeclarationOnly --allowJs --outDir dist"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "typescript": "^4.9.3",
    "vite": "^4.2.0"
  },
  "description": "state-sync-ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tamaka365/state-sync-ts.git"
  },
  "bugs": {
    "url": "https://github.com/tamaka365/state-sync-ts/issues"
  },
  "homepage": "https://github.com/tamaka365/state-sync-ts#readme"
}
